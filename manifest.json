{
  "manifest_version": 3,
  "name": "Chartink To TradingView",
  "version": "1.8",
  "description": "Redirects Chartink Symbol Links to TradingView Chart",
  "icons": {
    "48": "icons/logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.chartink.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "permissions": [
    "clipboardWrite",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  }
}